{
  "version": "1.0.0-*",
  "exclude": [
    "wwwroot"
  ],
  "publishExclude": [
    "**.kproj",
    "**.user",
    "**.vspscc"
  ],
  "dependencies": {
    "Microsoft.AspNet.Server.Kestrel": "1.0.0-*",
    "Microsoft.AspNet.WebUtilities": "1.0.0-*"
  },
  "frameworks": {
    "dnx451": {
      "frameworkAssemblies": {
        "System.Net.Http": ""
      }
    },
    "dnxcore50": {
      "dependencies": {
        "System.Net.Http": "4.0.1-*"
      }
    }
  },
  "compilationOptions": {
    "emitEntryPoint": true
  },
  "commands": {
    "web": "Content.Upload.Multipart"
  }
}
